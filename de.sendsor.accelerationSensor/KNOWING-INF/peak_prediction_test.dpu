<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataProcessingUnit name="peak-prediction-test">
    <description>Peak Prediction Test DPU</description>
    <tags>Peak Prediction</tags>
    <nodes>                
        <node nodeType="loader" factoryId="weka.core.converters.ArffLoader" id="ARFF">
            <properties>
                <item value="testdata.arff" key="file"/>
            </properties>
        </node>
        
        <node nodeType="processor" factoryId="de.sendsor.accelerationSensor.algorithm.moennig.preprocessing.TruncatedPeakPrediction" id="TruncatedPeakPrediction">
            <properties>            
            	<item key="normalize" value="true" />
            	<item key="minValue" value="-128" />
            	<item key="maxValue" value="127" />
            	<item key="normalizationRange" value="2.0" />            	            	
            </properties>
        </node>
        
        <node nodeType="presenter" factoryId="de.lmu.ifi.dbs.knowing.core.swt.TablePresenter" id="TablePresenter">
            <properties>
                <item value="100" key="rows"/>
            </properties>
        </node>
    </nodes>
    <edges>    	
        <edge weight="1" targetId="TruncatedPeakPrediction" sourceId="ARFF" id="arff_validator"/>        
		<edge weight="1" targetId="TablePresenter" sourceId="TruncatedPeakPrediction" id="validator_presenter"/>
    </edges>
</DataProcessingUnit>