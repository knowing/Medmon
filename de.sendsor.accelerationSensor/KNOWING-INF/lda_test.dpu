<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DataProcessingUnit name="LDA-Test">
    <description>LDA Test</description>
    <tags>LDA</tags>
    <nodes>                
        <node nodeType="loader" factoryId="weka.core.converters.ArffLoader" id="ARFF">
            <properties>
                <item value="testdata/ldatestdata.arff" key="file"/>
            </properties>
        </node>
       
        
        <node nodeType="processor" factoryId="de.sendsor.accelerationSensor.algorithm.moennig.lda.LDAFilter" id="LDA">
            <properties>            
            	<item key="debug" value="true"/>   
            	<item key="dimensionreduction" value="false" />
            	<item key="attributeNamePrefix" value="" />
            </properties>
        </node>                
        
        <node nodeType="presenter" factoryId="de.lmu.ifi.dbs.knowing.core.swt.TablePresenter" id="TablePresenter">
            <properties>
                <item value="100" key="rows"/>
            </properties>
        </node>
    </nodes>
    <edges>    	
        <edge weight="1" targetId="LDA" sourceId="ARFF" id="arff_validator"/>                     
		<edge weight="1" targetId="TablePresenter" sourceId="LDA" id="validator_presenter"/>
    </edges>
</DataProcessingUnit>