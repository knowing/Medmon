<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            icon="icons/alt_window_16.gif"
            visible="true">
         <run
               class="de.lmu.ifi.dbs.medmon.medic.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.lmu.ifi.dbs.medmon.medic.rcp.application"
            name="Medmon for Medics">
         <property
               name="appName"
               value="Medmon for Medics">
         </property>
         <property
               name="aboutImage"
               value="medmon.trans.png">
         </property>
         <property
               name="aboutText"
               value="Medmon Medic App">
         </property>
         <property
               name="windowImages"
               value="icons/alt_window_16.gif,icons/alt_window_32.gif">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
